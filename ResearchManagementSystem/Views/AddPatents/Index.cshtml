@model IEnumerable<ResearchManagementSystem.Models.AddPatent>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ProductionId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PatentNo)
            </th>

            <th>
                @Html.DisplayNameFor(model => model.ApplicationFormFileName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.AddAccomplishment.ProductionId)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.PatentNo)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ApplicationFormFileName)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.patentId">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.patentId">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.patentId">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
